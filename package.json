{
  "name": "defscript",
  "version": "0.0.2",
  "description": "A simple, solid compile-to-js language with a extension API",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "node --experimental-modules test/index.js",
    "repl": "node --experimental-modules test/repl.js",
    "build": "rollup -c",
    "build-parser": "crane lib/grammar.crane -r module > lib/parser.out.mjs",
    "build-parsers": "crane lib/grammar.crane -r e > lib/expression-parser.mjs && crane lib/grammar.crane -r module > lib/module-parser.mjs"
  },
  "bin": {
    "defscript": "./bin/defscript.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielratener/defscript.git"
  },
  "keywords": [
    "web",
    "javascript",
    "compile-to-js"
  ],
  "author": "Gabriel Ratener",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielratener/defscript/issues"
  },
  "homepage": "https://github.com/gabrielratener/defscript#readme",
  "dependencies": {
    "commander": "^2.18.0",
    "crane-parser": "^0.1.7",
    "estemplate": "^0.5.1",
    "lexie": "^1.0.5"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "livescript": "^1.5.0"
  }
}
